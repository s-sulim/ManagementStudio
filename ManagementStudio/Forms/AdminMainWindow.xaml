<Window x:Class="ManagementStudio.Forms.AdminMainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ManagementStudio.Forms"
        xmlns:userControls="clr-namespace:ManagementStudio.UserControls"
        mc:Ignorable="d"
        Title="AdminMainWindow" Height="450" Width="800" Closed="Window_Closed">
    <Grid>
        <TabControl>
            <TabItem Header="Students">
                <userControls:StudentsGridView></userControls:StudentsGridView>
            </TabItem>
            <TabItem Header="Users">
                <userControls:UsersGridView></userControls:UsersGridView>
            </TabItem>
        </TabControl>
    </Grid>
</Window>
